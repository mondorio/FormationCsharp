        <TextBox x:Name="InputCPT" HorizontalAlignment="Left" Margin="99,243,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" Height="22" Background="#FFD2D0D0"/>

        <Button Content="Ajouter" Margin="465,237,0,0" HorizontalAlignment="Left" VerticalAlignment="Top" Click="Ajouter_click" Width="180" Background="#FFDDDDDD"/>
        <Button Content="Retour" Margin="68,335,0,0" HorizontalAlignment="Left" VerticalAlignment="Top" Click="Retour_Click" Width="180" Background="#FFDDDDDD"/>

